! terminal colors ------------------------------------------------------------

! tangoesque scheme
*background: #222222
*foreground: #dedede
! Black
*color0: #000000
*color8: #555753
! Red
*color1: #ff6565
*color9: #ff8d8d
! Green
*color2: #93d44f
*color10: #c8e7a8
! Yellow
*color3: #eab93d
*color11: #ffc123
! Blue
*color4: #204a87
*color12: #3465a4
! Mangenta
*color5: #ce5c00
*color13: #f57900
! Cyan
*color6: #89b6e2
*color14: #46a4ff
! White
*color7: #cccccc
*color15: #ffffff

! rxvt-unicode ---------------------------------------------------------------
urxvt*font: xft:Bitstream Vera Sans Mono:pixelsize=11,\
            xft:Baekmuk Gulim
 
urxvt*boldFont: xft:Bitstream Vera Sans Mono Bold:pixelsize=11,\
            xft:Baekmuk Gulim

urxvt*geometry: 160x50
!urxvt*internalBorder: 5
urxvt*inheritPixmap: false
!urxvt*depth: 24
urxvt*depth: 32
urxvt*saveLines: 32767
!urxvt*visualBell: true
urxvt*scrollTtyKeypress: true
urxvt*scrollWithBuffer: false
urxvt*scrollTtyOutput: false
urxvt*scrollBar: false
!urxvt*scrollstyle: plain
!urxvt*scrollColor: #777777
!urxvt*cursorColor: #ffcc00
!urxvt*loginShell: true
urxvt*cutchars: "()*,<>[]{}|"
!urxvt*print-pipe: cat > $(echo urxvt.dump.`date +'%Y%M%d%H%m%S'`)
urxvt*secondaryScroll: true
!urxvt*mapAlert: true
!urxvt*utmpInhibit: true
urxvt*perl-lib: /usr/lib/urxvt/perl/
urxvt*background: rgba:0000/0000/0000/ffff

urxvt*perl-ext: selection,mark-yank-urls
urxvt*termName: rxvt-unicode
urxvt*urlLauncher: /usr/bin/firefox
urxvt*matcher.button: 1
urxvt*perl-ext-common: default,matcher,tabbed

Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
 
urxvt.loginShell: true
urxvt.boldMode: false
urxvt.xftAntialias: false
urxvt.jumpScroll: true
urxvt.tintColor: white
URxvt*internalBordercolor: #121212
urxvt.cursorBlink: false
urxvt.cursorColor: #ff8939
urxvt.cursorColor2: #121212
urxvt.colorBD: #857672
urxvt.colorIT: #bbbbbb
urxvt.colorUL: #999999
urxvt.underlineColor: #999999
urxvt.borderless: true
